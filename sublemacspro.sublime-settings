{
  /* Allows to always switch to active mark mode */
  "sbp_active_mark_mode" : false,

  /* Regular expression for word navigation */
  "sbp_word_separators" : "./\\()\"'-_:,.;<>~!@#$%^&*|+=[]{}`~?",

